<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="redmine_project_tree" model="ir.ui.view">
            <field name="name">redmine.project.tree</field>
            <field name="model">redmine.project</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Projects">
                    <field name="project_title"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="view_buttons_form">
            <field name="name">Buttons</field>
            <field name="model">redmine.project</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Button record">
                    <button string="Sync projects" type="object" name="sync_projects" class="oe_highlight"/>
                </form>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_view_users">
            <field name="name">Users</field>
            <field name="res_model">redmine.project</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="domain">[]</field>
        </record>
        <menuitem id="menu_redmine" name="Redmine"/>
        <menuitem id="redmine_project" name="Redmine Projects" parent="menu_redmine"
                  action="action_view_users"/>
    </data>
</odoo>